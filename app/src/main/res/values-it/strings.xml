    <!--OPERATORS UNIQUE GADGET DESCRIPTION-->
    <string name="ugat_ash">M120 MIC</string>
    <string name="uga_ash">Spara un colpo dirompente che penetra la superficie ed esplode automaticamente.</string>

    <string name="ugat_bandit">Generatore da campo CED-1</string>
    <string name="uga_bandit">Piazza cavi ad alta tensione sui gadget metallici, che infliggono danni al nemico.</string>

    <string name="ugat_blackbeard">TARS MK O-Scudo d\'assalto rinforzato</string>
    <string name="uga_blackbeard">Ha uno scudo di riserva per sostituire il primo in caso venga danneggiato.</string>

    <string name="ugat_blitz">Scudo tattico G52</string>
    <string name="uga_blitz">Attiva una flashbang integrata sullo scudo balistico che acceca temporaneamente i nemici.</string>

    <string name="ugat_buck">SK 4–12</string>
    <string name="uga_buck">Attiva uno shotgun agganciato sotoo la canna dell\'arma principale.</string>

    <string name="ugat_capitao">TAC Mk0</string>
    <string name="uga_capitao">Balestra tattica che scaglia dardi asfissianti e microgranate fumogene.</string>

    <string name="ugat_castle">Pannello tattico universale PTU1</string>
    <string name="uga_castle">Piazza delle barricate antiproiettile, più resistenti di quelle standard.</string>

    <string name="ugat_caveira">Passo felpato</string>
    <string name="uga_caveira">II talento di muoversi come un predatore, il "passo felpato", consente a Caveira di avvicinarsi ai nemici senza che se ne accorgano.</string>

    <string name="ugat_doc">Siringa automatica MPD-O</string>
    <string name="uga_doc">Spara un dardo per rianimare se stesso o un compagno a distanza. Può anche conferire un bonus temporaneo.</string>

    <string name="ugat_echo">YOKAI</string>
    <string name="uga_echo">Drone volante in grado di liberare scariche acustiche. Yokai trasmette inoltre immagini video all\'operatore.</string>

    <string name="ugat_frost">Sterling MK2 LHT</string>
    <string name="uga_frost">Usa una trappola meccanica per bloccare i nemici.</string>

    <string name="ugat_fuze">APM-6 "Matryoshka"</string>
    <string name="uga_fuze">Usa cariche a grappolo adesive che perforano le pareti e liberano granate multiple dal lato opposto.</string>

    <string name="ugat_glaz">Ottica mobile HDS</string>
    <string name="uga_glaz">Monta un mirino ottico con un sofisticato sensore d\'immagine, che delinea i bersagli a lunga distanza anche attraverso il fumo.</string>

    <string name="ugat_hibana">X-KAIROS</string>
    <string name="uga_hibana">Lanciagranate con cariche da 40 mm, dotato di proiettili esplosivi che possono essere detunati a distanza.</string>

    <string name="ugat_iq">RED MK III "Spectre"</string>
    <string name="uga_iq">Usa un rilevatore portatile per localizzare dispositivi nel suo raggio d\'azione, nonostante gli ostacoli.</string>

    <string name="ugat_jackal">EYENOX MODELLO III</string>
    <string name="uga_jackal">Occhiali traccianti in grado di individuare i bersagli rilevando e identificando impronte recanti.</string>

    <string name="ugat_jager">ADS-MKIV "Magpie"</string>
    <string name="uga_jager">Usa un sistema di difesa attiva per intercettare le granate prima dello scoppio.</string>

    <string name="ugat_kapkan">EDD MK II</string>
    <string name="uga_kapkan">Piazza trappole su finestre e porte.</string>

    <string name="ugat_mira">SPECCHIO NERO</string>
    <string name="uga_mira">Piazza un vetro a specchio antiproiettile su pareti fragili e rinforzate. Può anche essere aperto per creare una feritoia.</string>

    <string name="ugat_montagne">Le Roc</string>
    <string name="uga_montagne">Può estendere lo scudo per proteggere completamente i compagni quando è in piedi.</string>

    <string name="ugat_mute">Jammer GC90 "Moni"</string>
    <string name="uga_mute">Blocca i segnali radio dei gadget esplosivi a distanza e dei droni nel suo raggio d\'azione.</string>

    <string name="ugat_pulse">Sensore cardiaco HB-5</string>
    <string name="uga_pulse">Rileva il battito cardiaco a breve distanza attraverso gli ostacoli.</string>

    <string name="ugat_rook">Corazza R1N "Rhino"</string>
    <string name="uga_rook">Mette a disposizione della squadra una sacca piena di piastre corazzate.</string>

    <string name="ugat_sledge">Martello tattico da sfondamento "Caber"</string>
    <string name="uga_sledge">Usa un martello da sfondamento per fare breccia nelle superfici che possono essere distrutte.</string>

    <string name="ugat_smoke">Granata al composto Z8</string>
    <string name="uga_smoke">Piazza una granata tossica con detonatore a distanza.</string>

    <string name="ugat_tachanka">Mitragliatrice RP-46 Degtyaryov</string>
    <string name="uga_tachanka">Piazza una mitragliatrice fissa con uno scudo balistico che protegge la testa dagli attacchi frontali.</string>

    <string name="ugat_thatcher">Granata EMP EG MK0</string>
    <string name="uga_thatcher">Usa una granata EMP che disattiva i dispositivi elettronici nel suo raggio di azione.</string>

    <string name="ugat_thermite">Carica esotermica BRIMSTONE BC-3</string>
    <string name="uga_thermite">Piazza una carica esotermica in grado di distruggere le pareti rinforzate.</string>

    <string name="ugat_twitch">Drone stordente RSD Model 1</string>
    <string name="uga_twitch">Usa due droni stordenti per disattivare le trappole o ferire i nemici. Schiera il primo drone stordente durante la fase preparatoria.</string>

    <string name="ugat_valkyrie">Telecamera giroscopica Mk2</string>
    <string name="uga_valkyrie">Piazza delle telecamere Black Eye, a cui può accedere tutta la squadra con gli strumenti di osservazione.</string>

    <string name="ugat_ying">Candela</string>
    <string name="uga_ying">Un grappolo di flashbang che può essere ancorato alle superfici o fatto rotolare.</string>

    <string name="ugat_lesion">Gu</string>
    <string name="uga_lesion">Mine tossiche mimetiche che iniettano un composto in grado di ferire e limitare la velocità dei nemici.</string>

    <string name="ugat_ela">Grzmot Mine</string>
    <string name="uga_ela">Mine di prossimità a concussione che possono essere applicate alle superfici: danneggiano l\'udito e hanno un effetto stordente.</string>

    <string name="ugat_zofia">KS79 LIFELINE</string>
    <string name="uga_zofia">Double barrel that can fire both concussion and impact grenades.</string>

    <string name="ugat_dokkaebi">LOGIC BOMB</string>
    <string name="uga_dokkaebi">Uploads a virus that forces enemy devices to emit a loud buzz, thereby compromising their position.</string>

    <string name="ugat_vigil">ELECTRONIC RENDERING CLOAK (ERC-7)</string>
    <string name="uga_vigil">Vigil scans surrounding devices to wipe his image from cameras and drones.</string>

